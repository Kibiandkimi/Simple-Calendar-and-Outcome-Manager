      <VBox>
         <children>
            <HBox fx:id="_itemHBox" focusTraversable="true">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <children>
                  <CheckBox mnemonicParsing="false">
                     <HBox.margin>
